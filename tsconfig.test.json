{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./test-dist",
    "rootDir": ".",
    "declaration": false,
    "declarationMap": false,
    "incremental": false,
    "tsBuildInfoFile": null,
    "noEmit": true
  },
  "include": [
    "src/**/*",
    "test/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "test-dist"
  ]
}
